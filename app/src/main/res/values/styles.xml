    <style name="EditTextLabel">
        <item name="android:textSize">@dimen/_10asdp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">@color/color_CDCED2</item>
        <item name="textStyle">@integer/sfpro_display_regular</item>
        <item name="android:paddingStart">@dimen/_5asdp</item>
        <item name="android:layout_marginTop">@dimen/_10asdp</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="EditTextProfileLabel">
        <item name="android:textSize">@dimen/_12asdp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">@color/color_4A4A4A</item>
        <item name="textStyle">@integer/helvetica_neue_medium</item>
        <item name="android:layout_marginTop">@dimen/_10asdp</item>
    </style>

    <style name="EditTextProfile">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_12asdp</item>
        <item name="android:textColor">@color/color_707070</item>
        <item name="android:textColorHint">@color/color_9FA3A5</item>
        <item name="textStyle">@integer/sfpro_display_regular</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_marginTop">@dimen/_10asdp</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="TextViewMore">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_12asdp</item>
        <item name="android:textColor">@color/color_4A4A4A</item>
        <item name="textStyle">@integer/helvetica_neue_medium</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>



    <style name="EditTextQuestionnaireLabel">
        <item name="android:textSize">@dimen/_10asdp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:textColor">@color/color_4A4A4A</item>
        <item name="textStyle">@integer/sfpro_display_regular</item>
        <item name="android:layout_marginTop">@dimen/_10asdp</item>
    </style>

    <style name="ButtonWhite">
        <item name="android:textSize">@dimen/_10asdp</item>
        <item name="android:layout_height">@dimen/_40asdp</item>
        <item name="android:background">@drawable/ic_button_bg</item>
        <item name="android:textColor">@color/color_8895B3</item>
        <item name="textStyle">@integer/font_style_medium</item>
    </style>

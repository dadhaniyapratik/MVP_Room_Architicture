<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="DialogAnimationUpDown">
        <item name="android:windowEnterAnimation">@anim/dialog_up</item>
        <item name="android:windowExitAnimation">@anim/dialog_down</item>
    </style>

    <style name="DialogAnimationAlpha">
        <item name="android:windowEnterAnimation">@anim/alpha_enter</item>
        <item name="android:windowExitAnimation">@anim/alpha_exit</item>
    </style>

    <style name="DialogAnimationAlphaFast">
        <item name="android:windowEnterAnimation">@anim/alpha_enter_fast</item>
        <item name="android:windowExitAnimation">@anim/alpha_exit_fast</item>
    </style>

    <style name="FitAppBarLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:transitionName" tools:targetApi="lollipop">@string/transition_appbar
        </item>
        <item name="android:transitionGroup" tools:targetApi="lollipop">true</item>
    </style>

    <style name="Toolbar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">?actionBarSize</item>
        <item name="titleTextColor">@color/colorPrimaryDark</item>
        <item name="subtitleTextColor">@android:color/darker_gray</item>
        <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
        <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    </style>






    <!--<item name="android:background">@drawable/dark_btn_bg_selector</item>-->

    <style name="DialogStyle" parent="Base.Theme.AppCompat.Dialog">
        <item name="android:windowMinWidthMajor">90%</item>
        <item name="android:windowMinWidthMinor">90%</item>
        <item name="android:background">@android:color/transparent</item>
    </style>

    <style name="DialogThemeTNC">
        <item name="android:windowEnterAnimation">@anim/slide_right</item>
        <item name="android:windowExitAnimation">@anim/slide_left</item>
    </style>

    <style name="DialogThemePrivacyPolicy">
        <item name="android:windowEnterAnimation">@anim/slide_left</item>
        <item name="android:windowExitAnimation">@anim/slide_right</item>
    </style>

    <style name="Button">
        <item name="android:textSize">@dimen/_10asdp</item>
        <item name="android:layout_height">@dimen/_40asdp</item>
        <item name="android:background">@drawable/ic_btn_bg_red</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="textStyle">@integer/font_style_medium</item>
    </style>

